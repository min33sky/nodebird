{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["airbnb"],
  "rules": {
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "no-multi-spaces": "off",
    "indent": "off",
    "no-trailing-spaces": "off",
    "comma-dangle": "off"
  }
}
